{
    "manifest_version": 3,
    "name": "Online Meeting Toolkit",
    "description": "Some handy tools for online meeting",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": [
                "https://*.notion.so/*",
                "https://docs.google.com/presentation/*"
            ],
            "js": ["index.js"],
            "css": ["assets/index.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["assets/musics/*.mp3"],
            "matches": ["<all_urls>"]
        }
    ]
}
